{
  "plugins": [
    "addcssjs",
    "richquotes",
    "prism",
    "-hightlight",
    "anchors",
    "js-sequence-diagram-full@>=0.3.1",
    "splitter",
    "github",
    "anchor-navigation-ex",
    "expandable-chapters-small",
    "pageview-count",
    "language-picker",
    "hide-element"
  ],
  "pluginsConfig": {
    "addcssjs": {
      "js": [
        "../scripts/mermaid.min.js",
        "../scripts/index.js"
      ],
      "css": [
      ]
    },
    "prism": {
      "css": [
        "prismjs/themes/prism.css"
      ],
      "lang": {
        "java": "java"
      },
      "ignore": [
        "sequence"
      ]
    },
    "theme-default": {
      "showLevel": false
    },
    "github": {
      "url": "https://github.com/TuyaInc/tuyasmart_bizbundle_android_doc"
    },
    "anchor-navigation-ex": {
      "showGoTop": true
    },
    "hide-element": {
            "elements": [".gitbook-link"]
    }
  }
}